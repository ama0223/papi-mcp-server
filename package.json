{
  "name": "segment-public-api",
  "version": "58.14.0",
  "description": "MCP Server generated from OpenAPI spec for segment-public-api",
  "private": true,
  "type": "module",
  "main": "build/index.js",
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc && chmod 755 build/index.js",
