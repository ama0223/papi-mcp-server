    "prestart": "npm run build",
    "clean": "rm -rf build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "axios": "^1.8.4",
    "express": "^5.1.0",
    "json-schema-to-zod": "^2.6.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^18.19.0",
    "typescript": "^5.4.5"
  }
}
